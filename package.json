{
  "name": "gatsby-starter-template",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-starter-template",
  "keywords": [
    "gatsby"
  ],
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx}": [
      "prettier",
      "eslint",
      "git add"
    ]
  },
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby clean && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "compile": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier:fix": "prettier --write  ./**/*.{js,ts,tsx}"
  },
  "dependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "dotenv": "^10.0.0",
    "gatsby": "^3.14.2",
    "gatsby-background-image": "^1.5.3",
    "gatsby-plugin-emotion": "^6.14.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^2.0.0",
    "gatsby-plugin-layout": "^3.0.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-react-helmet": "^5.0.0",
    "gatsby-plugin-sharp": "^4.0.0",
    "gatsby-plugin-sitemap": "^4.10.0",
    "gatsby-plugin-theme-ui": "^0.12.0",
    "gatsby-source-contentful": "^6.0.0",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-transformer-sharp": "^4.0.0",
    "gbimage-bridge": "^0.1.4",
    "hamburger-react": "^2.4.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.20.2",
    "react-icons": "^4.3.1",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "theme-ui": "^0.12.0"
  },
  "devDependencies": {
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.3",
    "prettier": "2.4.1"
  }
